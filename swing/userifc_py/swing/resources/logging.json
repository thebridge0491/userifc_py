{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "fmtr2": {
            "format":
                "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        },
        "fmtr1": {
            "format": "%(asctime)s %(levelname)s:%(name)s:%(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "hdlr2": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "fmtr2",
            "stream": "ext://sys.stderr"
        },
        "hdlr1": {
            "class": "logging.FileHandler",
            "level": "DEBUG",
            "formatter": "fmtr1",
            "filename": "root.log",
            "mode": "a"
        },
        "hdlr3": {
            "class": "logging.FileHandler",
            "level": "DEBUG",
            "formatter": "fmtr2",
            "filename": "prac.log",
            "mode": "a"
        }
    },
    "loggers": {
        "practice": {
            "level": "DEBUG",
            "handlers": ["hdlr3"],
            "propagate": "yes"
        }
    },
    "root": {
        "level": "INFO",
        "handlers": ["hdlr1", "hdlr2"]
    }
}
